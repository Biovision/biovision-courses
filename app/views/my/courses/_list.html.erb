<% if collection.any? %>
  <ul class="list-of-courses">
    <% collection.each do |entity| %>
      <li data-id="<%= entity.id %>">
        <%= render partial: 'my/courses/course_user/in_list', locals: { entity: entity } %>
      </li>
    <% end %>
  </ul>
<% else %>
  <%= render 'shared/nothing_found' %>
<% end %>
